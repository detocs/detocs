{
  "name": "detocs",
  "productName": "DETOCS",
  "version": "0.6.0-rc.4",
  "description": "Datagram's Esports Tournament Overlay Control System",
  "author": "Adrian Mullings",
  "license": "AGPL-3.0",
  "repository": "github:detocs/detocs",
  "homepage": "https://github.com/detocs/detocs",
  "files": [
    "build/*"
  ],
  "type": "commonjs",
  "main": "build/src/main.js",
  "bin": {
    "detocs": "build/src/main.js"
  },
  "engines": {
    "node": ">=12.18.0"
  },
  "scripts": {
    "tsc": "tsc && tsc-alias",
    "webjs": "rollup -c -m",
    "webjs-analyze": "rollup -c -m --environment ANALYZE",
    "screenshotjs": "tsc --project ./screenshots && tsc-alias",
    "webicons": "svg-sprite --symbol --dest build/public/icons --symbol-sprite icons.svg node_modules/heroicons/outline/dots-circle-horizontal.svg node_modules/heroicons/outline/x-circle.svg",
    "build": "gulp",
    "prestart": "npm run build",
    "start": "node build/src/main.js",
    "electron": "electron .",
    "electron-debug": "electron-builder --dir",
    "electron-archive": "node scripts/archive-electron-win.js",
    "electron-release": "electron-builder && npm run electron-archive",
    "pkg": "pkg --out-path dist/server . && node scripts/pkg-rename.js",
    "dist": "npm run pkg && npm run electron-release",
    "license-check": "license-checker --summary --failOn 'UNKNOWN; UNLICENSED'",
    "postinstall": "patch-package && npm run license-check",
    "test": "jest --testPathIgnorePatterns='\\.it\\.'",
    "test-integration": "jest \\.it\\.",
    "test-all": "npx jest",
    "screenshots": "npm run build screenshots && node build/screenshots/main.js"
  },
  "build": {
    "appId": "com.datagram.detocs",
    "files": [
      "build/**/*"
    ],
    "directories": {
      "output": "dist/full"
    },
    "icon": "build/public/images/detocs.png",
    "win": {
      "artifactName": "${name}-${version}.${ext}",
      "target": "nsis"
    },
    "nsis": {
      "artifactName": "${name}-${version}_Setup.${ext}",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "pkg": {
    "assets": [
      "build/public/**/*",
      "build/templates/**/*"
    ]
  },
  "dependencies": {
    "@google/youtube": "git+https://github.com/data-enabler/googleapis-youtube.git",
    "chainable-error": "^1.0.1",
    "chokidar": "^3.5.1",
    "cors": "^2.8.5",
    "csv-parse": "^4.8.8",
    "express": "^4.17.1",
    "express-formidable": "^1.2.0",
    "fast-xml-parser": "^3.17.4",
    "filenamify": "^4.1.0",
    "find-process": "^1.4.2",
    "front-matter": "^3.0.2",
    "google-auth-library": "^6.0.6",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "handlebars": "^4.7.6",
    "immutability-helper": "^3.0.1",
    "isomorphic-fetch": "^3.0.0",
    "js-yaml": "^3.13.1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.countby": "^4.6.0",
    "lodash.debounce": "^4.0.8",
    "lodash.escaperegexp": "^4.1.2",
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "lodash.mergewith": "^4.6.2",
    "lodash.range": "^3.2.0",
    "lodash.set": "^4.3.2",
    "lodash.sortedindex": "^4.1.0",
    "lodash.throttle": "^4.1.1",
    "log4js": "^4.5.1",
    "micro-memoize": "^4.0.8",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.31",
    "neverthrow": "^4.0.1",
    "node-youtube-resumable-upload": "git+https://github.com/data-enabler/node-youtube-resumable-upload.git",
    "oauth-1.0a": "^2.2.6",
    "obs-websocket-js": "^3.1.0",
    "open": "^7.0.3",
    "p-limit": "^3.0.2",
    "twit": "^2.2.11",
    "ws": "^6.2.1",
    "yargs": "^14.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "@rollup/plugin-replace": "^2.3.3",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/express-formidable": "^1.0.4",
    "@types/jest": "^28.1.3",
    "@types/js-yaml": "^3.12.2",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.countby": "^4.6.6",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.escaperegexp": "^4.1.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.merge": "^4.6.6",
    "@types/lodash.mergewith": "^4.6.6",
    "@types/lodash.range": "^3.2.6",
    "@types/lodash.set": "^4.3.6",
    "@types/lodash.sortedindex": "^4.1.6",
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^12.12.54",
    "@types/react": "^16.9.38",
    "@types/react-toast-notifications": "^2.4.0",
    "@types/twit": "^2.2.22",
    "@types/twitter-text": "^2.0.0",
    "@types/ws": "^6.0.3",
    "@types/yargs": "^13.0.3",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "archiver": "^5.0.0",
    "browserslist": "^4.14.0",
    "clsx": "^1.1.1",
    "core-js": "^3.6.5",
    "cpx2": "^2.0.0",
    "doiuse": "^4.4.1",
    "electron": "^9.4.4",
    "electron-builder": "^22.8.0",
    "electron-packager": "^15.0.0",
    "element-qsa-scope": "^1.1.0",
    "eslint": "^7.9.0",
    "eslint-plugin-react": "^7.20.6",
    "form-request-submit-polyfill": "^2.0.0",
    "gulp": "^4.0.2",
    "gulp-run": "^1.7.1",
    "heroicons": "^0.4.2",
    "jest": "^28.1.1",
    "license-checker": "^25.0.1",
    "normalize.css": "^8.0.1",
    "patch-package": "^6.2.2",
    "pkg": "^4.4.9",
    "postcss": "^7.0.32",
    "postcss-input-range": "^4.0.0",
    "postcss-preset-env": "^6.7.0",
    "preact": "^10.0.0-rc.2",
    "puppeteer": "^13.0.1",
    "react-toastify": "^6.0.6",
    "reconnecting-websocket": "^4.4.0",
    "rollup": "^2.18.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-visualizer": "^4.0.4",
    "sass": "^1.26.5",
    "svg-sprite": "^1.5.0",
    "ts-jest": "^28.0.5",
    "tsc-alias": "^1.1.1",
    "twitter-text": "^3.1.0",
    "typescript": "^4.5.4",
    "w3c-keys": "^1.0.1"
  }
}
